<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.spaceprogram.accounting.basic.InvoiceTemplate" table="ACCTInvoiceTemplates">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="companyKey"/>
        <property name="name"/>
        <property name="logo"/>
        <property name="companyInfo"/>
        <property name="topText"/>
        <property name="bottomText"/>

    </class>


</hibernate-mapping>
